<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.myauto.designer.mehanic_brig_rab"
    tools:showIn="@layout/activity_mehanic_brig_rab">

    <TextView
        android:text="Рабочие операции выполняемые моей бригадой"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textStyle="bold"
        android:padding="8dp"
        android:textAlignment="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView132" />

    <ListView
        android:id="@+id/mehanic_brig_rab_listview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView132">

    </ListView>

</RelativeLayout>
